{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "heyvl",
      "camelcase": "HeyVL",
      "title": "HeyVL",
      "scope": "source.heyvl",
      "file-types": [
        "heyvl"
      ],
      "injection-regex": "^heyvl$",
      "class-name": "TreeSitterHeyvl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Grammar for HeyVL, the Intermediate Verification Language (IVL) used by the Caesar Verifier",
    "authors": [
      {
        "name": "kernzerfall",
        "email": "me@kernzerfall.eu",
        "url": "https://github.com/kernzerfall"
      }
    ],
    "links": {
      "repository": "https://github.com/kernzerfall/tree-sitter-heyvl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
